{
  "name": "cli-kids",
  "version": "0.1.0",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "esmodule",
      "in-source": true
    }
  ],
  "suffix": ".bs.js",
  "jsx": {
    "version": 4,
    "mode": "automatic"
  },
  "bs-dependencies": ["@rescript/react", "@rescript/core"],
  "warnings": {
    "error": "+101"
  },
  "bsc-flags": ["-open RescriptCore"]
}
